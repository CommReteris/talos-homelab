        "calibre-web",
        "kubernetes-sigs/external-dns",
        "zigbee2mqtt",
        "unifi-poller",
        "tautulli",
        "speedtest-exporter",
        "sabnzbd",
        "qbittorrent",
        "node-red",
        "node-red",
        "mosquitto",
        "kah-common-chart",
        "jellyfin",
        "intel-gpu-plugin"
      ]
    }
  ]
